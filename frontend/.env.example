# Frontend Environment Configuration

# API Configuration
NUXT_PUBLIC_STRAPI_URL=http://localhost:1337
NUXT_PUBLIC_API_BASE_URL=http://localhost:1337/api

# Agent System Configuration
NUXT_PUBLIC_AGENT_WEBSOCKET_URL=ws://localhost:1337/agent-ws
NUXT_PUBLIC_AGENT_API_URL=http://localhost:1337/api/agents
NUXT_PUBLIC_ENABLE_AGENT_CHAT=true
NUXT_PUBLIC_ENABLE_AGENT_MONITORING=true

# Prefect Configuration
NUXT_PUBLIC_PREFECT_UI_URL=http://localhost:4200
NUXT_PUBLIC_ENABLE_WORKFLOW_MONITORING=true

# Feature Flags
NUXT_PUBLIC_ENABLE_AI_SEARCH=true
NUXT_PUBLIC_ENABLE_LEARNING_PATHS=true
NUXT_PUBLIC_ENABLE_ADAPTIVE_LEARNING=true
NUXT_PUBLIC_ENABLE_ANALYTICS_DASHBOARD=true
NUXT_PUBLIC_ENABLE_MULTI_AGENT_CHAT=true

# UI Configuration
NUXT_PUBLIC_APP_NAME=EduLMS v2
NUXT_PUBLIC_APP_DESCRIPTION=Intelligent Learning Management System
NUXT_PUBLIC_DEFAULT_LOCALE=en
NUXT_PUBLIC_SUPPORTED_LOCALES=en,es,fr,de

# Performance Configuration
NUXT_PUBLIC_ENABLE_PWA=true
NUXT_PUBLIC_ENABLE_SERVICE_WORKER=true
NUXT_PUBLIC_CACHE_TTL=300

# Analytics Configuration
NUXT_PUBLIC_ENABLE_USER_ANALYTICS=true
NUXT_PUBLIC_ANALYTICS_ENDPOINT=/api/analytics/track

# Real-time Configuration
NUXT_PUBLIC_WEBSOCKET_RECONNECT_INTERVAL=5000
NUXT_PUBLIC_WEBSOCKET_MAX_RETRIES=5
NUXT_PUBLIC_ENABLE_REAL_TIME_UPDATES=true

# Development Configuration
NUXT_PUBLIC_DEBUG_MODE=false
NUXT_PUBLIC_MOCK_AGENT_RESPONSES=false
NUXT_PUBLIC_ENABLE_DEV_TOOLS=true