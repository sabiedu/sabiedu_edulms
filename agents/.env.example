# Agent System Environment Configuration

# Database Configuration (TiDB)
DATABASE_HOST=your-tidb-host
DATABASE_PORT=4000
DATABASE_NAME=edulms_v2
DATABASE_USERNAME=your-username
DATABASE_PASSWORD=your-password
DATABASE_SSL=true
DATABASE_MAX_CONNECTIONS=20
DATABASE_POOL_SIZE=10

# LLM Services Configuration
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
OPENAI_MAX_TOKENS=4000
OPENAI_TEMPERATURE=0.7

# AWS Bedrock Configuration (Alternative)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0

# Prefect Configuration
PREFECT_API_URL=http://localhost:4200/api
PREFECT_SERVER_HOST=localhost
PREFECT_SERVER_PORT=4200
PREFECT_WORKSPACE=default
PREFECT_WORK_POOL=default-agent-pool

# Agent Configuration
MASTER_AGENT_NAME=master-agent
CONTENT_CURATOR_AGENT_NAME=content-curator-agent
LEARNING_PATH_AGENT_NAME=learning-path-agent
ASSESSMENT_AGENT_NAME=assessment-agent
TUTOR_AGENT_NAME=tutor-agent
RESEARCH_AGENT_NAME=research-agent
ANALYTICS_AGENT_NAME=analytics-agent

# Agent Behavior Configuration
AGENT_MAX_RETRIES=3
AGENT_RETRY_DELAY=1.0
AGENT_TIMEOUT_SECONDS=300
AGENT_BATCH_SIZE=10
AGENT_POLLING_INTERVAL=1.0

# Cache Configuration
CACHE_TTL_DEFAULT=3600
CACHE_TTL_EMBEDDINGS=86400
CACHE_TTL_ANALYTICS=1800
CACHE_TTL_SESSIONS=86400
CACHE_CLEANUP_INTERVAL=300

# Message Queue Configuration
MESSAGE_RETENTION_DAYS=30
MESSAGE_BATCH_SIZE=100
MESSAGE_POLLING_INTERVAL=1.0
MESSAGE_MAX_RETRIES=3

# Task Queue Configuration
TASK_RETENTION_DAYS=7
TASK_BATCH_SIZE=50
TASK_POLLING_INTERVAL=2.0
TASK_MAX_RETRIES=3
TASK_PRIORITY_LEVELS=5

# Session Management
SESSION_TIMEOUT_MINUTES=60
SESSION_CLEANUP_INTERVAL=600
MAX_CONCURRENT_SESSIONS=1000

# Performance Configuration
MAX_CONCURRENT_AGENTS=10
AGENT_MEMORY_LIMIT_MB=512
AGENT_CPU_LIMIT_PERCENT=50

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE_PATH=logs/agents.log
LOG_MAX_SIZE_MB=100
LOG_BACKUP_COUNT=5
ENABLE_METRICS=true
METRICS_PORT=8080

# Security Configuration
ENABLE_AGENT_AUTH=true
AGENT_API_KEY=your-agent-api-key
ENCRYPT_AGENT_COMMUNICATIONS=true
AGENT_RATE_LIMIT_PER_MINUTE=100

# Vector Search Configuration
VECTOR_DIMENSION=1536
VECTOR_SIMILARITY_THRESHOLD=0.8
VECTOR_INDEX_TYPE=IVF_FLAT
VECTOR_NPROBE=10

# External Services
STRAPI_API_URL=http://localhost:1337/api
STRAPI_API_TOKEN=your-strapi-api-token

# Development Configuration
DEBUG_MODE=false
ENABLE_AGENT_PROFILING=false
MOCK_LLM_RESPONSES=false
AGENT_STARTUP_DELAY=0